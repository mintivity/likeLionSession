public class Person {
    private float tall;
    private float weight;

    Person(float tall, float weight){
        this.tall = tall;
        this.weight = weight;
    }
    public float getBMI(){
        return weight/(tall*tall)*10000;
    }
    public float getTall(){
        return tall;
    }
    public float getWeight(){
        return weight;
    }

    public void say(){
        System.out.println("응애");
    }
}
